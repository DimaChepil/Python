class Symetric(np.ndarray):

    def __setitem__(self, index, value):

        super(Symetric,self).__setitem__(index,value)

        super(Symetric,self).__setitem__(index,value)

        

def symetric(Z):

    return np.asarray(Z+Z.T - np.diag(Z.diagonal())).view(Symetric)

